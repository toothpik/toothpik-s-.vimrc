#!/usr/bin/perl
#  last fiscal year
#  or, more accurately, last year's worth of fiscally completed months
$ty = `date '+%y'`;
chop($ty);
$py = $ty - 1;
$tm = `cfm`;
chop($tm);
int($tm);

$pyp = "m" . $py;

foreach $m ($tm .. 12) {
    $cypher = `$pyp $m`;
    print $cypher;
}

if ($tm > 1) {
    $typ = "m" . $ty;
    foreach $m (1 .. $tm - 1) {
        $cypher = `$typ $m`;
        print $cypher;
    }
}
