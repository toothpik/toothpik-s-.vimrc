#!/usr/bin/python3
#  fix "Today" in weather stream (reads stdin), used by ccw
#  also fixes "Labor Day" which is always a Monday
#  ditto "Columbus Day"
from re import compile
from datetime import date
from sys import stdin

tod = date.today().strftime("%A")

s0 = "Today"
s1 = "This Afternoon"
s2 = "Late Afternoon"
s3 = "Labor Day"
s4 = "Columbus Day"

l0 = compile(s0)
l1 = compile(s1)
l2 = compile(s2)
l3 = compile(s3)
l4 = compile(s4)

for line in stdin:
    l = line.rstrip()
    if l0.search(l):
        nl = tod + l.lstrip(s0)
    elif l1.search(l):
        nl = tod + " afternoon " + l.lstrip(s1)
    elif l2.search(l):
        nl = tod + " afternoon " + l.lstrip(s2)
    elif l3.search(l):
        nl = 'Monday' + l.lstrip(s3)
    elif l4.search(l):
        nl = 'Monday' + l.lstrip(s4)
    else:
        nl = l
    print(nl)
